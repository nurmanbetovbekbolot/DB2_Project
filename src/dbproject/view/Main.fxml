<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="dbproject.controller.MainController">
    <children>
        <TabPane prefHeight="600.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Benutzer">
                    <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <BorderPane prefHeight="571.2" prefWidth="800.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0">
                                    <left>
                                        <TableView fx:id="userTable" prefHeight="571.2" prefWidth="800.0"
                                                   BorderPane.alignment="CENTER">
                                            <columns>
                                                <TableColumn fx:id="vorNameColumn" prefWidth="75.0" text="Vorname"/>
                                                <TableColumn fx:id="nachNameColumn" prefWidth="75.0" text="Nachname"/>
                                                <TableColumn fx:id="loginColumn" prefWidth="75.0" text="Login"/>
                                                <TableColumn fx:id="createdAtColumn" prefWidth="75.0" text="Erstellt_am"/>
                                            </columns>
                                        </TableView>
                                    </left>
                                    <bottom>
                                        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                            <buttons>
                                                <Button fx:id="createUserButton" onAction="#handleCreateUser"
                                                        mnemonicParsing="false" text="Benutzer erstellen"/>
                                            </buttons>
                                            <buttons>
                                                <Button fx:id="editUserButton" onAction="#handleEditUser"
                                                        mnemonicParsing="false" text="Benutzer bearbeiten"/>
                                            </buttons>

                                            <buttons>
                                                <Button fx:id="deleteUserButton" onAction="#handleDeleteUser"
                                                        mnemonicParsing="false" text="Benutzer löschen"/>
                                            </buttons>
                                        </ButtonBar>
                                    </bottom>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Paket">
                    <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <BorderPane prefHeight="571.2" prefWidth="800.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0">
                                    <left>
                                        <TableView fx:id="packageTable" prefHeight="571.2" prefWidth="800.0"
                                                   BorderPane.alignment="CENTER">
                                            <columns>
                                                <TableColumn fx:id="packageNameColumn" prefWidth="75.0" text="Name"/>
                                                <TableColumn fx:id="descriptionColumn" prefWidth="75.0"
                                                             text="Bezeihnung"/>
                                                <TableColumn fx:id="preisColumn" prefWidth="75.0" text="Preis"/>
                                                <TableColumn fx:id="discountPreisColumn" prefWidth="119.20001220703125"
                                                             text="Discount Preis"/>
                                            </columns>
                                        </TableView>
                                    </left>
                                    <bottom>
                                        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                            <buttons>
                                                <Button fx:id="createPaketButton" onAction="#handleCreatePaket"
                                                        mnemonicParsing="false" text="Paket erstellen"/>
                                            </buttons>
                                            <buttons>
                                                <Button fx:id="editPaketButton" onAction="#handleEditPaket"
                                                        mnemonicParsing="false" text="Paket bearbeiten"/>
                                            </buttons>

                                            <buttons>
                                                <Button fx:id="deletePaketButton" onAction="#handleDeletePaket"
                                                        mnemonicParsing="false" text="Paket löschen"/>
                                            </buttons>
                                        </ButtonBar>
                                    </bottom>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Bestellung">
                    <content>
                        <BorderPane prefHeight="200.0" prefWidth="200.0"/>
                    </content>
                </Tab>
                <Tab text="e">
                    <content>
                        <MenuBar>
                            <menus>
                                <Menu mnemonicParsing="false" text="File">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="Close"/>
                                    </items>
                                </Menu>
                                <Menu mnemonicParsing="false" text="Edit">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="Delete"/>
                                    </items>
                                </Menu>
                                <Menu mnemonicParsing="false" text="Help">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="About"/>
                                    </items>
                                </Menu>
                            </menus>
                        </MenuBar>
                    </content>
                </Tab>
                <Tab text="re">
                    <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <BorderPane prefHeight="571.2" prefWidth="800.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0">
                                    <left>
                                        <TableView fx:id="packageTable1" prefHeight="571.2" prefWidth="800.0"
                                                   BorderPane.alignment="CENTER">
                                            <columns>
                                                <TableColumn fx:id="packageNameColumn1" prefWidth="75.0" text="Name"/>
                                                <TableColumn fx:id="descriptionColumn1" prefWidth="75.0"
                                                             text="Bezeihnung"/>
                                                <TableColumn fx:id="preisColumn1" prefWidth="75.0" text="Preis"/>
                                                <TableColumn fx:id="discountPreisColumn1" prefWidth="119.20001220703125"
                                                             text="Discount Preis"/>
                                            </columns>
                                        </TableView>
                                    </left>
                                    <bottom>
                                        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                            <buttons>
                                                <Button mnemonicParsing="false" text="Button"/>
                                            </buttons>
                                        </ButtonBar>
                                    </bottom>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
